{
  "id": null,
  "uid": "cloud-benchmarking",
  "title": "K6 Cloud Kubernetes Benchmarking",
  "tags": ["k6", "prometheus", "gke", "eks", "aks"],
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "30s",
  "templating": {
    "list": [
      {
        "name": "cluster",
        "type": "query",
        "datasource": "Prometheus",
        "query": "label_values(kube_node_info, cluster)",
        "current": { "text": "gke", "value": "gke" }
      },
      {
        "name": "namespace",
        "type": "query",
        "datasource": "Prometheus",
        "query": "label_values(kube_pod_info, namespace)",
        "current": { "text": "boutique", "value": "boutique" }
      }
    ]
  },
  "panels": [
    {
      "type": "graph",
      "title": "Requests per Second (RPS)",
      "targets": [
        {
          "expr": "sum(rate(http_requests_total{namespace=\"$namespace\"}[1m]))",
          "legendFormat": "RPS",
          "refId": "A"
        }
      ],
      "gridPos": { "x": 0, "y": 0, "w": 12, "h": 8 }
    },
    {
      "type": "graph",
      "title": "Latency p95 and p99",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket{namespace=\"$namespace\"}[1m])) by (le))",
          "legendFormat": "p95",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(http_request_duration_seconds_bucket{namespace=\"$namespace\"}[1m])) by (le))",
          "legendFormat": "p99",
          "refId": "B"
        }
      ],
      "gridPos": { "x": 12, "y": 0, "w": 12, "h": 8 }
    },
    {
      "type": "graph",
      "title": "Error Rate (%)",
      "targets": [
        {
          "expr": "100 * (sum(rate(http_requests_failed_total{namespace=\"$namespace\"}[1m])) / sum(rate(http_requests_total{namespace=\"$namespace\"}[1m])))",
          "legendFormat": "Error %",
          "refId": "A"
        }
      ],
      "gridPos": { "x": 0, "y": 8, "w": 12, "h": 6 }
    },
    {
      "type": "graph",
      "title": "CPU Usage per Node",
      "targets": [
        {
          "expr": "instance:node_cpu_utilisation:rate1m{cluster=\"$cluster\"}",
          "legendFormat": "{{instance}}",
          "refId": "A"
        }
      ],
      "gridPos": { "x": 12, "y": 8, "w": 12, "h": 6 }
    },
    {
      "type": "graph",
      "title": "Memory Usage per Node (%)",
      "targets": [
        {
          "expr": "100 * ((node_memory_MemTotal_bytes - node_memory_MemAvailable_bytes) / node_memory_MemTotal_bytes)",
          "legendFormat": "{{instance}}",
          "refId": "A"
        }
      ],
      "gridPos": { "x": 0, "y": 14, "w": 12, "h": 6 }
    },
    {
      "type": "graph",
      "title": "Frontend Replica Count",
      "targets": [
        {
          "expr": "count(kube_pod_info{namespace=\"$namespace\", pod=~\"frontend.*\"})",
          "legendFormat": "Frontend Pods",
          "refId": "A"
        }
      ],
      "gridPos": { "x": 12, "y": 14, "w": 12, "h": 6 }
    },
    {
      "type": "graph",
      "title": "Cluster Node Count",
      "targets": [
        {
          "expr": "count(kube_node_info{cluster=\"$cluster\"})",
          "legendFormat": "Nodes",
          "refId": "A"
        }
      ],
      "gridPos": { "x": 0, "y": 20, "w": 12, "h": 6 }
    },
    {
      "type": "graph",
      "title": "Ingress / Egress Network Traffic (bytes/s)",
      "targets": [
        {
          "expr": "sum(rate(container_network_receive_bytes_total{namespace=\"$namespace\"}[1m]))",
          "legendFormat": "Ingress",
          "refId": "A"
        },
        {
          "expr": "sum(rate(container_network_transmit_bytes_total{namespace=\"$namespace\"}[1m]))",
          "legendFormat": "Egress",
          "refId": "B"
        }
      ],
      "gridPos": { "x": 12, "y": 20, "w": 12, "h": 6 }
    },
    {
    "type": "graph",
    "title": "Application Node Count (label: application)",
    "targets": [
        {
        "expr": "count(kube_node_labels{label_application!=\"\", cluster=\"$cluster\"})",
        "legendFormat": "Nodes with label 'application'",
        "refId": "A"
        }
    ],
    "gridPos": { "x": 12, "y": 26, "w": 12, "h": 6 }
    }
  ]
}
